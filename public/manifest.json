{
  "name": "IAUTabler",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Creates tables from PeopleSoft's search section.",
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "file:///*",
        "*://sis.iau.edu.sa/psc/*/EMPLOYEE/SA/c/*"
      ],
      "all_frames": true,
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {}
}
