{
  "name": "IAUTabler",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Create tables from PeopleSoft's search section.",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "file:///*",
        "*://sis.iau.edu.sa/psc/*/EMPLOYEE/SA/c/*"
      ],
      "all_frames": true,
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {},
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
